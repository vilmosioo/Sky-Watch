server {
    listen  <%= ENV['OPENSHIFT_NGINX_IP'] %>:<%= ENV['OPENSHIFT_NGINX_PORT'] %>;

		location ~* \.(ico|txt|js|jpg|gif|png|css|html)$ {
        root    <%= ENV['OPENSHIFT_REPO_DIR'] %>dist;
        expires 30d;
    }

		location / {
    		proxy_pass <%= ENV['OPENSHIFT_NGINX_IP'] %>:15000;
    }
}